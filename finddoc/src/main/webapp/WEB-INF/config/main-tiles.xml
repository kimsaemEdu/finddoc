<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="indexTemplate"
		template="/WEB-INF/layout/index.jsp">
		<put-attribute name="top"
			value="/WEB-INF/include/top.jsp" />
		<put-attribute name="content"
			value="/WEB-INF/include/main_content.jsp" />
	</definition>

	<definition name="mainTemplate"
		template="/WEB-INF/layout/main.jsp">
		<put-attribute name="top"
			value="/WEB-INF/include/top.jsp" />
		<put-attribute name="menu"
			value="/WEB-INF/menu/admin_page_menu.jsp" />
		<put-attribute name="content"
			value="/WEB-INF/admin/accept_list.jsp" />
	</definition>

	<!-- 첫 화면 페이지 -->
	<definition name="index" extends="indexTemplate">
	</definition>


</tiles-definitions>
	